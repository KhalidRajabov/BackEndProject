@model HomeVM
@{
    ViewData["Title"] = "Home Page";
}

    <section  class="slider-area slider-active">
        @foreach (var item in Model.Sliders)
       {
           <div class="single-slider">
            <div class="slider-image">
                <img src="~/images/@item.ImageUrl" alt="slider">
            </div>
            <div class="slider-content-wrapper">
                <div class="container-fluid custom-container">
                    <div class="slider-content">
                        <h4 data-animation="fadeInDown" data-delay="0.5s" class="sub-title">@Html.Raw(item.Subtitle)</h4>
                        <h1 data-animation="fadeInLeft" data-delay="0.5s" class="main-title ">@Html.Raw(item.MainTitle)</h1>
                        <p data-animation="fadeInRightBig" data-delay="0.5s">@item.Description</p>
                        <a data-animation="zoomIn" data-delay="0.5s" class="main-btn " href="single-product.html">Shop Now</a>
                    </div>
                </div> <!-- container -->
            </div>
        </div> <!-- single slider -->
       }
    </section>

    <!--====== Slider Part Ends ======-->

    <!--====== Banner Part Start ======-->

    <div class="banner-area banner-bg">
        <div class="container-fluid custom-container">
            <div class="row">
                <div class="col-md-6">
                    <div class="banner-image">
                        <a href="shop-4-column.html"><img src="~/images/banner-1.png" alt="banner"></a>
                    </div> <!-- banner image -->
                </div>
                <div class="col-md-6">
                    <div class="banner-image">
                        <a href="shop-4-column.html"><img src="~/images/banner-2.png" alt="banner"></a>
                    </div> <!-- banner image -->
                </div>
            </div> <!-- row -->
        </div> <!-- container -->
    </div>

    <!--====== Banner Part Ends ======-->

    <!--====== Category Part Start ======-->

    <section class="category-area category-bg pt-100 pb-100">
        <div class="container-fluid custom-container">
            <div class="row justify-content-center">
                <div class="col-lg-6">
                    <div class="section-title text-center pb-30">
                        <h2 class="title">Featured Categories</h2>
                    </div>
                </div>
            </div> <!-- row -->
               <div class="row category-active">
            @foreach (var item in Model.Categories)
           {
                <div class="col-lg-2">
                    <div class="single-category text-center">
                        <a target="_blank" href="shop-left-sidebar.html">
                            <img src="~/images/@item.ImageUrl"  width="148px" height="148px" alt="category">
                            <p>@item.Name</p>
                        </a>
                    </div> <!-- single category --> <!-- single category -->
                </div>
           }
            </div> <!-- row -->
        </div> <!-- container -->
    </section>

    <!--====== Category Part Ends ======-->

    <!--====== Product Part Start ======-->

