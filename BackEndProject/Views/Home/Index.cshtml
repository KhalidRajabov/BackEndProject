                                </div> <!-- products gallery image -->
                            </div> <!-- special products gallery -->
                        </div>
                        <div class="col-lg-6">
                            <div class="special-products-countdown mt-50">
                                <div class="products-countdown-wrapper">
                                    <h2 class="special-title">Deal of the week</h2>
                                    <h3 class="products-title"><a href="#">@item.Name...</a></h3>
                                    <div class="products-rating">
                                        <ul>
                                            <li><i class="fas fa-star"></i></li>
                                            <li><i class="fas fa-star"></i></li>
                                            <li><i class="fas fa-star"></i></li>
                                            <li><i class="fas fa-star"></i></li>
                                            <li><i class="fas fa-star"></i></li>
                                        </ul>
                                    </div>

                                    <div data-countdown="2022/12/19"></div>

                                    <div class="product-price-and-cart d-sm-flex align-items-center">
                                        <div class="product-price">
                                            <span class="price-sale">€@item.DiscountPrice</span>
                                            <span class="regular-price">€@item.Price</span>
                                        </div>
                                        <div class="product-cart">
                                            <a class="main-btn main-btn-2" asp-action="additem" asp-controller="basket" asp-route-id="@item.Id" asp-route-ReturnUrl="@Context.Request.Path">Add to cart</a>
                                        </div>
                                    </div>
                                </div> <!-- special products countdown -->
                            </div> <!-- special products countdown -->
                        </div>
                    </div> <!-- row -->
                </div> <!-- special products wrapper -->
                  
               }
                
            </div> <!-- special products active -->
