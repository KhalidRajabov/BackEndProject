                                    <ul class="products-thumb">
                                        @foreach (var image in item.ProductImages)
                                       {
                                           <li><img src="~/images/product/@image.ImageUrl" width="120" height="90" alt="Products Thumb"></li>
                                       }
                                        
                                    </ul>
                                </div> <!-- products gallery thumb -->
                                <div class="products-gallery-image">
                                    @foreach (var image in item.ProductImages)
